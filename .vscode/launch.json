{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Next.js dev server",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "serverReadyAction": {
        "pattern": "- Local:\\s+(http://localhost:9002)",
        "uriFormat": "%s",
        "action": "openExternally"
      }
    }
  ]
}
